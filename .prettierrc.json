{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "jsxBracketSameLine": true,
    "importOrder": [
        "^@(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "experimentalBabelParserPluginsList": [
        "jsx",
        "typescript"
    ]
}